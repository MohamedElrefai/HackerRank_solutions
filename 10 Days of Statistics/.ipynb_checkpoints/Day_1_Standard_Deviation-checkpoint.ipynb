   "source": [
    "import math\n",
    "num=int(input())\n",
    "numbers=input()\n",
    "numbers=list(map(int,numbers.split(' ')))\n",
    "lgth=len(numbers)\n",
    "summing=0\n",
    "#mean\n",
    "mean=sum(numbers)/lgth\n",
    "#standard deviation \n",
    "for i in numbers:\n",
    "    summing+=(i-mean)**2\n",
    "std_dev=round(math.sqrt((summing)/lgth),1)\n",
    "print(std_dev)"
   ]
